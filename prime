#include <stdio.h>
void input(int *n)
{
	printf("enter the number");
	scanf("%d",n);
}
void compute_output(int n)
{
	int i,j,fact;
	for(i=2;i<=n;i++)
	{
		fact=1;
		for(j=2;j<=i/2;j++)
		{
			if(i%j==0)
			{
				fact=0;
			}
		}
			if(fact==1)
			{
				printf("%d \n",i);
			}
		
	}

}
void main()
{
	int n;
	input(&n);
	compute_output(n);
	
}
